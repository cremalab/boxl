{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "jsRules": {},
  "rules": {
    "prettier": [
      true,
      {
        "trailingComma": "es5"
      }
    ],
    "no-implicit-dependencies": false,
    "trailing-comma": [
      true,
      "spec"
    ],
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "rulesDirectory": []
}